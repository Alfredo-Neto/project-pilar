    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.3.0",
    "bootstrap-vue": "^2.23.1",
    "clipboard": "^2.0.11",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lint-staged": "^13.2.3",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
